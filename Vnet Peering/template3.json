{
     "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
     "contentVersion": "1.0.0.0",
     "parameters": {
        "vNet1Name":{"type":"string"},
        "vNet2Name":{"type":"string"}

     },
     "variables": {
     },
 "resources": [
         {
         "apiVersion": "2016-06-01",
         "type": "Microsoft.Network/virtualNetworks/virtualNetworkPeerings",
         "name": "[concat(parameters('vNet1Name'),'/Assignmnet')]",
         "location": "[resourceGroup().location]",
         "properties": {
         "allowVirtualNetworkAccess": true,
         "allowForwardedTraffic": false,
         "allowGatewayTransit": false,
         "useRemoteGateways": false,
             "remoteVirtualNetwork": {
             "id": "[concat('/subscriptions/5a4e2629-2885-4a52-b513-ecbe4a3c035f/resourceGroups/MyAzureRsrcGrp/providers/Microsoft.Network/virtualNetworks/',parameters('vNet2Name'))]"
             }
         }
         },
             {
         "apiVersion": "2016-06-01",
         "type": "Microsoft.Network/virtualNetworks/virtualNetworkPeerings",
         "name": "[concat(parameters('vNet2Name'),'/Assignmnet')]",
         "location": "[resourceGroup().location]",
         "properties": {
         "allowVirtualNetworkAccess": true,
         "allowForwardedTraffic": false,
         "allowGatewayTransit": false,
         "useRemoteGateways": false,
             "remoteVirtualNetwork": {
              "id": "[concat('/subscriptions/5a4e2629-2885-4a52-b513-ecbe4a3c035f/resourceGroups/MyAzureRsrcGrp/providers/Microsoft.Network/virtualNetworks/',parameters('vNet1Name'))]"
             }
         }
         }

     ]
}